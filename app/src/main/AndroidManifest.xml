<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vcarart.digimondex">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_digimon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_digimon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Digimondex">
        <activity android:name="com.vcarart.digimondex.ui.digimonList.DigimonListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vcarart.digimondex.ui.digimonInfo.DigimonInfoActivity" />
    </application>

</manifest>